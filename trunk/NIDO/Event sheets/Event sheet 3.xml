<?xml version="1.0" encoding="utf-8" ?>
<c2eventsheet>
    <!--All the 'name' attributes are ignored by Construct 2 - they are there for readability only.-->
    <name>Event sheet 3</name>
    <events>
        <event-block>
            <conditions>
                <condition disabled="1" id="0" name="On collision with another object" type="huevo">
                    <param id="0" name="Object">Camino1</param>
                </condition>
                <condition id="2" name="Key is down" type="Keyboard">
                    <param id="0" name="Key">68 (D)</param>
                </condition>
            </conditions>
            <actions>
                <action behavior="Physics" disabled="1" id="0" name="Apply force" type="huevo">
                    <param id="0" name="Force X">8</param>
                    <param id="1" name="Force Y">0</param>
                    <param id="2" name="Image point">0</param>
                </action>
                <action behavior="Physics" id="3" name="Apply impulse" type="huevo">
                    <param id="0" name="Impulse X">0.2</param>
                    <param id="1" name="Implulse Y">0</param>
                    <param id="2" name="Image point">0</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="2" name="Key is down" type="Keyboard">
                    <param id="0" name="Key">65 (A)</param>
                </condition>
            </conditions>
            <actions>
                <action behavior="Physics" id="0" name="Apply force" type="huevo">
                    <param id="0" name="Force X">-30</param>
                    <param id="1" name="Force Y">0</param>
                    <param id="2" name="Image point">0</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="2" name="Key is down" type="Keyboard">
                    <param id="0" name="Key">82 (R)</param>
                </condition>
            </conditions>
            <actions>
                <action id="-18" name="Rotate clockwise" type="huevo">
                    <param id="0" name="Degrees">10</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="2" name="Key is down" type="Keyboard">
                    <param id="0" name="Key">76 (L)</param>
                </condition>
            </conditions>
            <actions />
        </event-block>
        <event-block>
            <conditions>
                <condition id="0" name="On collision with another object" type="spider">
                    <param id="0" name="Object">Camino1</param>
                </condition>
            </conditions>
            <actions />
        </event-block>
        <event-block>
            <conditions>
                <condition id="-1" name="Every tick" type="System" />
            </conditions>
            <actions>
                <action id="-4" name="Set position to another object" type="Agua">
                    <param id="0" name="Object">MiniGota</param>
                    <param id="1" name="Image point (optional)">0</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-6" name="Every X seconds" type="System">
                    <param id="0" name="Interval (seconds)">0.5</param>
                </condition>
            </conditions>
            <actions>
                <action behavior="Physics" id="0" name="Apply force" type="MiniGota">
                    <param id="0" name="Force X">random(-1,1)</param>
                    <param id="1" name="Force Y">random(-1,1)</param>
                    <param id="2" name="Image point">0</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="0" name="On collision with another object" type="huevo">
                    <param id="0" name="Object">Lucia</param>
                </condition>
            </conditions>
            <actions>
                <action id="4" name="Set animation" type="huevo">
                    <param id="0" name="Animation">&quot;Animation2&quot;</param>
                    <param id="1" name="From">1</param>
                </action>
                <action id="-15" name="Set visible" type="EggOver">
                    <param id="0" name="Visibility">1</param>
                </action>
                <action behavior="Flash" id="0" name="Flash" type="EggOver">
                    <param id="0" name="&apos;On&apos; time">0.1</param>
                    <param id="1" name="&apos;Off&apos; time">0.1</param>
                    <param id="2" name="Duration">1.0</param>
                </action>
            </actions>
        </event-block>
        <event-group description="" title="Hierva">
            <sub-events>
                <event-block>
                    <conditions>
                        <condition disabled="1" id="-11" name="For Each" type="System">
                            <param id="0" name="Object">Hierva</param>
                        </condition>
                        <condition id="-1" name="Every tick" type="System" />
                    </conditions>
                    <actions>
                        <action behavior="Sin" id="1" name="Set period" type="Hierva">
                            <param id="0" name="Period">Hierva.v</param>
                        </action>
                        <action behavior="Sin2" id="1" name="Set period" type="Hierva">
                            <param id="0" name="Period">Hierva.v</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="-6" name="Every X seconds" type="System">
                            <param id="0" name="Interval (seconds)">0.25</param>
                        </condition>
                        <condition id="-7" name="Compare instance variable" type="Hierva">
                            <param id="0" name="Instance variable">v</param>
                            <param id="1" name="Comparison">2</param>
                            <param id="2" name="Value">10</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action id="-11" name="Add to" type="Hierva">
                            <param id="0" name="Instance variable">v</param>
                            <param id="1" name="Value">0.25</param>
                        </action>
                    </actions>
                </event-block>
            </sub-events>
        </event-group>
        <event-block>
            <conditions>
                <condition id="1" name="Is overlapping another object" type="Hierva">
                    <param id="0" name="Object">huevo</param>
                </condition>
            </conditions>
            <actions>
                <action id="-10" name="Set value" type="Hierva">
                    <param id="0" name="Instance variable">v</param>
                    <param id="1" name="Value">1</param>
                </action>
            </actions>
        </event-block>
    </events>
</c2eventsheet>
