<?xml version="1.0" encoding="utf-8" ?>
<c2eventsheet>
    <!--All the 'name' attributes are ignored by Construct 2 - they are there for readability only.-->
    <name>Event sheet 1</name>
    <events>
        <event-group description="" title="Control">
            <sub-events>
                <event-block>
                    <conditions>
                        <condition id="2" name="Key is down" type="Keyboard">
                            <param id="0" name="Key">68 (D)</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action behavior="Physics" id="3" name="Apply impulse" type="huevo">
                            <param id="0" name="Impulse X">0.09</param>
                            <param id="1" name="Implulse Y">0</param>
                            <param id="2" name="Image point">0</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="0" name="On key pressed" type="Keyboard">
                            <param id="0" name="Key">87 (W)</param>
                        </condition>
                        <condition id="9" name="Is overlapping at offset" type="huevo">
                            <param id="0" name="Object">Camino1</param>
                            <param id="1" name="Offset X">0</param>
                            <param id="2" name="Offset Y">2</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action behavior="Physics" id="3" name="Apply impulse" type="huevo">
                            <param id="0" name="Impulse X">0</param>
                            <param id="1" name="Implulse Y">-2</param>
                            <param id="2" name="Image point">0</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="2" name="Key is down" type="Keyboard">
                            <param id="0" name="Key">65 (A)</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action behavior="Physics" id="3" name="Apply impulse" type="huevo">
                            <param id="0" name="Impulse X">-0.09</param>
                            <param id="1" name="Implulse Y">0</param>
                            <param id="2" name="Image point">0</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="2" name="Key is down" type="Keyboard">
                            <param id="0" name="Key">82 (R)</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action id="-18" name="Rotate clockwise" type="huevo">
                            <param id="0" name="Degrees">10</param>
                        </action>
                    </actions>
                </event-block>
            </sub-events>
        </event-group>
        <event-block>
            <conditions>
                <condition id="-1" name="Every tick" type="System" />
            </conditions>
            <actions>
                <action id="-1" name="Set X" type="Scroller">
                    <param id="0" name="X">huevo.X</param>
                </action>
                <action id="-2" name="Set Y" type="Scroller">
                    <param id="0" name="Y">huevo.Y-150</param>
                </action>
            </actions>
        </event-block>
        <event-group description="" title="PajaroLoco">
            <sub-events>
                <event-block>
                    <conditions>
                        <condition id="-2" name="On start of layout" type="System" />
                    </conditions>
                    <actions>
                        <action behavior="Pin" id="0" name="Pin to object" type="Lucia">
                            <param id="0" name="Pin to">Pajaro</param>
                        </action>
                        <action id="-10" name="Set value" type="Pajaro">
                            <param id="0" name="Instance variable">Xrelease</param>
                            <param id="1" name="Value">random(huevo.X,600)</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="-8" name="Compare two values" type="System">
                            <param id="0" name="First value">abs(Pajaro.X-Pajaro.Xrelease)</param>
                            <param id="1" name="Comparison">2</param>
                            <param id="2" name="Second value">10</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action behavior="Pin" id="1" name="Unpin" type="Lucia" />
                        <action behavior="Physics" id="21" name="Set immovable" type="Lucia">
                            <param id="0" name="Setting">0</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="-9" inverted="1" name="Is visible" type="Pajaro" />
                    </conditions>
                    <actions />
                </event-block>
            </sub-events>
        </event-group>
        <event-block>
            <conditions>
                <condition id="0" name="On collision with another object" type="Lucia">
                    <param id="0" name="Object">huevo</param>
                </condition>
            </conditions>
            <actions>
                <action id="-15" name="Set visible" type="EggOver">
                    <param id="0" name="Visibility">1</param>
                </action>
                <action id="4" name="Set animation" type="huevo">
                    <param id="0" name="Animation">&quot;Animation2&quot;</param>
                    <param id="1" name="From">1</param>
                </action>
            </actions>
        </event-block>
    </events>
</c2eventsheet>
