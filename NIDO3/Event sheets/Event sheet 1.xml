<?xml version="1.0" encoding="utf-8" ?>
<c2eventsheet>
    <!--All the 'name' attributes are ignored by Construct 2 - they are there for readability only.-->
    <name>Event sheet 1</name>
    <events>
        <variable name="scalelight" type="number">10</variable>
        <variable name="scalelight2" type="number">1</variable>
        <variable name="finfase" type="number">0</variable>
        <variable name="respawn" type="number">0</variable>
        <variable name="vidas" type="number">0</variable>
        <event-group description="Eventos OnStartofLayout" title="Eventos Iniciales">
            <sub-events>
                <event-block>
                    <conditions>
                        <condition id="-2" name="On start of layout" type="System" />
                    </conditions>
                    <actions>
                        <action id="7" name="Set mirrored" type="Sprite10">
                            <param id="0" name="State">0</param>
                        </action>
                    </actions>
                </event-block>
            </sub-events>
        </event-group>
        <event-group description="Controles del huevo" title="Control">
            <sub-events>
                <event-block>
                    <conditions>
                        <condition id="2" name="Is playing" type="huevo">
                            <param id="0" name="Animation">&quot;Default&quot;</param>
                        </condition>
                        <condition id="2" name="Key is down" type="Keyboard">
                            <param id="0" name="Key">68 (D)</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action behavior="Physics" id="3" name="Apply impulse" type="huevo">
                            <param id="0" name="Impulse X">0.09</param>
                            <param id="1" name="Implulse Y">0</param>
                            <param id="2" name="Image point">0</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="2" name="Key is down" type="Keyboard">
                            <param id="0" name="Key">65 (A)</param>
                        </condition>
                        <condition id="2" name="Is playing" type="huevo">
                            <param id="0" name="Animation">&quot;Default&quot;</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action behavior="Physics" id="3" name="Apply impulse" type="huevo">
                            <param id="0" name="Impulse X">-0.09</param>
                            <param id="1" name="Implulse Y">0</param>
                            <param id="2" name="Image point">0</param>
                        </action>
                    </actions>
                </event-block>
                <event-block collapsed="1">
                    <conditions>
                        <condition id="0" name="On key pressed" type="Keyboard">
                            <param id="0" name="Key">87 (W)</param>
                        </condition>
                    </conditions>
                    <actions />
                    <sub-events>
                        <event-block any="1">
                            <conditions>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">f2camino1</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">f2camino2</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">f2camino3</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">f2camino4</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">f2camino5</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">f2camino6</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">f2camino7</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">auxiliar</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">Sprite10</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">Sprite3</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">Sprite5</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">Sprite6</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">Sprite7</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">Sprite9</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                                <condition id="9" name="Is overlapping at offset" type="huevo">
                                    <param id="0" name="Object">trozof2camino5</param>
                                    <param id="1" name="Offset X">0</param>
                                    <param id="2" name="Offset Y">2</param>
                                </condition>
                            </conditions>
                            <actions>
                                <action behavior="Physics" id="3" name="Apply impulse" type="huevo">
                                    <param id="0" name="Impulse X">0</param>
                                    <param id="1" name="Implulse Y">-2</param>
                                    <param id="2" name="Image point">0</param>
                                </action>
                            </actions>
                        </event-block>
                    </sub-events>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="2" name="Key is down" type="Keyboard">
                            <param id="0" name="Key">82 (R)</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action id="-18" name="Rotate clockwise" type="huevo">
                            <param id="0" name="Degrees">10</param>
                        </action>
                    </actions>
                </event-block>
            </sub-events>
        </event-group>
        <event-block>
            <conditions>
                <condition id="0" name="On collision with another object" type="huevo">
                    <param id="0" name="Object">trozof2camino5</param>
                </condition>
            </conditions>
            <actions>
                <action behavior="Physics" id="21" name="Set immovable" type="trozof2camino5">
                    <param id="0" name="Setting">0</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-1" name="Every tick" type="System" />
            </conditions>
            <actions>
                <action id="-10" name="Set value" type="huevo">
                    <param id="0" name="Instance variable">Vel</param>
                    <param id="1" name="Value">max(huevo.Physics.VelocityX, huevo.Physics.VelocityY)</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-6" name="Every X seconds" type="System">
                    <param id="0" name="Interval (seconds)">1</param>
                </condition>
                <condition id="-14" name="Compare variable" type="System">
                    <param id="0" name="Variable">respawn</param>
                    <param id="1" name="Comparison">4</param>
                    <param id="2" name="Value">0</param>
                </condition>
            </conditions>
            <actions>
                <action id="-10" name="Add to" type="System">
                    <param id="0" name="Variable">respawn</param>
                    <param id="1" name="Value">1</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-14" name="Compare variable" type="System">
                    <param id="0" name="Variable">respawn</param>
                    <param id="1" name="Comparison">5</param>
                    <param id="2" name="Value">5</param>
                </condition>
            </conditions>
            <actions>
                <action id="-9" name="Set value" type="System">
                    <param id="0" name="Variable">respawn</param>
                    <param id="1" name="Value">0</param>
                </action>
                <action id="-26" name="Restart layout" type="System" />
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-1" name="Every tick" type="System" />
            </conditions>
            <actions>
                <action id="-9" name="Set value" type="System">
                    <param id="0" name="Variable">vidas</param>
                    <param id="1" name="Value">WebStorage.LocalValue(&quot;vidas&quot;)</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-14" name="Compare variable" type="System">
                    <param id="0" name="Variable">vidas</param>
                    <param id="1" name="Comparison">5</param>
                    <param id="2" name="Value">5</param>
                </condition>
            </conditions>
            <actions>
                <action id="7" name="Go to URL" type="Browser">
                    <param id="0" name="URL">&quot;http://localhost:50000&quot;</param>
                </action>
            </actions>
        </event-block>
        <event-block collapsed="1">
            <conditions>
                <condition id="-2" name="On start of layout" type="System" />
            </conditions>
            <actions>
                <action id="-9" name="Set value" type="System">
                    <param id="0" name="Variable">vidas</param>
                    <param id="1" name="Value">WebStorage.LocalValue(&quot;vidas&quot;)</param>
                </action>
            </actions>
            <sub-events>
                <event-block>
                    <conditions>
                        <condition id="-10" name="For" type="System">
                            <param id="0" name="Name">&quot;loop2&quot;</param>
                            <param id="1" name="Start index">0</param>
                            <param id="2" name="End index">vidas</param>
                        </condition>
                    </conditions>
                    <actions />
                    <sub-events>
                        <event-block>
                            <conditions>
                                <condition id="-13" name="Pick nth instance" type="System">
                                    <param id="0" name="Object">HuevoIntro4</param>
                                    <param id="1" name="Instance">loopindex(&quot;loop2&quot;)</param>
                                </condition>
                            </conditions>
                            <actions>
                                <action id="-9" name="Destroy" type="HuevoIntro4" />
                            </actions>
                        </event-block>
                    </sub-events>
                </event-block>
            </sub-events>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-1" name="Compare X" type="huevo">
                    <param id="0" name="Comparison">5</param>
                    <param id="1" name="X co-ordinate">7800</param>
                </condition>
            </conditions>
            <actions>
                <action id="-4" name="Set visible" type="System">
                    <param id="0" name="Layer">3</param>
                    <param id="1" name="Visibility">1</param>
                </action>
                <action id="-9" name="Set value" type="System">
                    <param id="0" name="Variable">finfase</param>
                    <param id="1" name="Value">1</param>
                </action>
                <action id="-16" name="Wait" type="System">
                    <param id="0" name="Seconds">3</param>
                </action>
                <action id="7" name="Go to URL" type="Browser">
                    <param id="0" name="URL">&quot;http://localhost:50004&quot;</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="-1" name="Every tick" type="System" />
            </conditions>
            <actions>
                <action id="-3" name="Set position" type="MouseLight">
                    <param id="0" name="X">huevo.X</param>
                    <param id="1" name="Y">huevo.Y</param>
                </action>
                <action id="-4" name="Set visible" type="System">
                    <param id="0" name="Layer">3</param>
                    <param id="1" name="Visibility">0</param>
                </action>
            </actions>
            <sub-events>
                <event-block>
                    <conditions>
                        <condition id="-14" name="Compare variable" type="System">
                            <param id="0" name="Variable">finfase</param>
                            <param id="1" name="Comparison">0</param>
                            <param id="2" name="Value">1</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action id="-4" name="Set visible" type="System">
                            <param id="0" name="Layer">3</param>
                            <param id="1" name="Visibility">1</param>
                        </action>
                        <action id="-9" name="Set value" type="System">
                            <param id="0" name="Variable">scalelight</param>
                            <param id="1" name="Value">scalelight*0.98</param>
                        </action>
                        <action id="9" name="Set scale" type="MouseLight">
                            <param id="0" name="Scale">scalelight</param>
                        </action>
                    </actions>
                </event-block>
                <event-block>
                    <conditions>
                        <condition id="-14" name="Compare variable" type="System">
                            <param id="0" name="Variable">scalelight2</param>
                            <param id="1" name="Comparison">2</param>
                            <param id="2" name="Value">10</param>
                        </condition>
                        <condition id="-14" name="Compare variable" type="System">
                            <param id="0" name="Variable">finfase</param>
                            <param id="1" name="Comparison">0</param>
                            <param id="2" name="Value">0</param>
                        </condition>
                        <condition disabled="1" id="-14" name="Compare variable" type="System">
                            <param id="0" name="Variable">vidas</param>
                            <param id="1" name="Comparison">0</param>
                            <param id="2" name="Value">0</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action id="-4" name="Set visible" type="System">
                            <param id="0" name="Layer">3</param>
                            <param id="1" name="Visibility">1</param>
                        </action>
                        <action id="-9" name="Set value" type="System">
                            <param id="0" name="Variable">scalelight2</param>
                            <param id="1" name="Value">scalelight2*1.02</param>
                        </action>
                        <action id="9" name="Set scale" type="MouseLight">
                            <param id="0" name="Scale">scalelight2</param>
                        </action>
                    </actions>
                </event-block>
            </sub-events>
        </event-block>
    </events>
</c2eventsheet>
